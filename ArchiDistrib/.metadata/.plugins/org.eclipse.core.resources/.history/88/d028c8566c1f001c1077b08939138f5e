package server.classes;

import java.rmi.RemoteException;
import java.rmi.server.UnicastRemoteObject;

import server.common.Animal;

public class AnimalImpl extends UnicastRemoteObject implements Animal{

	private String nom;
	private String maitre;
	private String espece;
	private String race;
	
	protected AnimalImpl(String nom, String maitre, String espece, String race) throws RemoteException {
		super();
		
		this.nom = nom;
		this.maitre = maitre;
		this.espece = espece;
		this.race = race;
	}
	
	
	@Override
	public String getNom() {
		System.out.println("Nom : " + nom);
		return nom;
	}

	@Override
	public String getNomMaitre() {
		System.out.println("Nom du maitre : " + maitre);
		return maitre;
	}

	@Override
	public String getEspece() {
		System.out.println("Espece : " + espece);
		return espece;
	}

	@Override
	public String getRace() {
		System.out.println("Race : " + race);
		return race;
	}
	
	

}
